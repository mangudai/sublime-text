{
    "name": "aoe2-rms",
    "displayName": "aoe2-rms",
    "description": "AoE2 Random Map Scripting support for VS Code and Sublime Text",
    "version": "0.1.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/mangudai/syntax-highlighting.git"
    },
    "homepage": "https://github.com/mangudai/syntax-highlighting",
    "bugs": {
        "url": "https://github.com/mangudai/syntax-highlighting/issues"
    },
    "license": "MIT",
    "publisher": "deltaidea",
    "engines": {
        "vscode": "^1.12.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "aoe2-rms",
            "aliases": ["AoE2 Random Map Script", "aoe2-rms"],
            "extensions": [".rms"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "aoe2-rms",
            "scopeName": "source.aoe2-rms",
            "path": "./aoe2-rms.tmLanguage"
        }]
    }
}
